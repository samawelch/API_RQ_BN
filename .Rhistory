add_row(NIPH_Sales_Weights_Summed %>% transmute(API_Name,
sYear,
Total_Sold_g,
Scenario = "Measured")) %>%
filter(API_Name %in% c("paracetamol", "ethinylestradiol"))
ggplot(data = Sales_Projections_Records,
mapping = aes(x = sYear,
y = Total_Sold_g,
colour = API_Name,
shape = Scenario)) +
geom_point()
